{
  "editor.fontSize": 15,
  "vscode-neovim.neovimExecutablePaths.darwin" : "/opt/homebrew/bin/nvim",
  "editor.accessibilitySupport" : "off",
  "workbench.editor.showTabs" : "single",
  "editor.fontFamily" : "JetBrains Mono",
  "vim.commandLineModeKeyBindings" : [

  ],
  "search.exclude" : {
    "**\/.metals" : true,
    "**\/target" : true,
    "**\/node_modules" : true,
    "**\/venv\/**" : true,
    "**\/project" : true,
    "**\/bower_components" : true,
    "**\/.bloop" : true,
    "**\/.git\/**" : true
  },
  "debug.console.lineHeight" : 13,
  "editor.minimap.enabled" : false,
  "files.watcherExclude" : {
    "**/.metals": true,
    "**/.git/subtree-cache/**": true,
    "**/.bloop": true,
    "**/.venv/**": true,
    "**/.direnv/**": true,
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/.ammonite": true
  },
  "vscode-neovim.highlightGroups.highlights" : {
    "Substitute" : "vim",
    "Visual" : {
      "backgroundColor" : "theme.editor.selectionBackground"
    },
    "IncSearch" : {
      "borderColor" : "theme.editor.findMatchBorder",
      "backgroundColor" : "theme.editor.findMatchBackground"
    },
    "Directory" : "vim",
    "Conceal" : "vim",
    "Search" : {
      "borderColor" : "theme.editor.findMatchHighlightBorder",
      "backgroundColor" : "theme.editor.findMatchHighlightBackground"
    }
  },
  "editor.fontLigatures" : true,
  "vscode-neovim.neovimInitVimPaths.linux" : "\/usr\/local\/bin\/nvim",
  "go.toolsManagement.autoUpdate" : true,
  "vscode-neovim.textDecorationsAtTop": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.formatting.provider": "none",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.lineHeight": 1.5,
  "git.openRepositoryInParentFolders": "never",
  "terminal.integrated.fontSize": 13
}