{
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "same",
  "editor.fontSize": 15,
  "vscode-neovim.neovimExecutablePaths.darwin" : "/opt/homebrew/bin/nvim",
  "editor.accessibilitySupport": "off",
  "editor.fontFamily" : "JetBrains Mono",
  "vim.commandLineModeKeyBindings" : [

  ],
  "search.exclude" : {
    "**\/.metals" : true,
    "**\/target" : true,
    "**\/node_modules" : true,
    "**\/venv\/**" : true,
    "**\/project" : true,
    "**\/bower_components" : true,
    "**\/.bloop" : true,
    "**\/.git\/**" : true
  },
  "debug.console.lineHeight" : 13,
  "editor.minimap.enabled" : false,
  "files.watcherExclude" : {
    "**/.metals": true,
    "**/.git/subtree-cache/**": true,
    "**/.bloop": true,
    "**/.venv/**": true,
    "**/.direnv/**": true,
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/.ammonite": true
  },
  "vscode-neovim.highlightGroups.highlights" : {
    "Substitute" : "vim",
    "Visual" : {
      "backgroundColor" : "theme.editor.selectionBackground"
    },
    "IncSearch" : {
      "borderColor" : "theme.editor.findMatchBorder",
      "backgroundColor" : "theme.editor.findMatchBackground"
    },
    "Directory" : "vim",
    "Conceal" : "vim",
    "Search" : {
      "borderColor" : "theme.editor.findMatchHighlightBorder",
      "backgroundColor" : "theme.editor.findMatchHighlightBackground"
    }
  },
  "editor.fontLigatures" : true,
  "vscode-neovim.neovimInitVimPaths.linux" : "\/usr\/local\/bin\/nvim",
  "go.toolsManagement.autoUpdate" : true,
  "vscode-neovim.textDecorationsAtTop": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.formatting.provider": "none",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.lineHeight": 1.5,
  "git.openRepositoryInParentFolders": "never",
  "terminal.integrated.fontSize": 14,
  "editor.lineNumbers": "off",
  "workbench.settings.applyToAllProfiles": [],
  "chat.instructionsFilesLocations": {
    ".github/instructions": true,
    "/var/folders/xt/6s00j8794qj1vznmb2b9q8lc0000gn/T/postman-collections-post-response.instructions.md": true,
    "/var/folders/xt/6s00j8794qj1vznmb2b9q8lc0000gn/T/postman-collections-pre-request.instructions.md": true,
    "/var/folders/xt/6s00j8794qj1vznmb2b9q8lc0000gn/T/postman-folder-post-response.instructions.md": true,
    "/var/folders/xt/6s00j8794qj1vznmb2b9q8lc0000gn/T/postman-folder-pre-request.instructions.md": true,
    "/var/folders/xt/6s00j8794qj1vznmb2b9q8lc0000gn/T/postman-http-request-post-response.instructions.md": true,
    "/var/folders/xt/6s00j8794qj1vznmb2b9q8lc0000gn/T/postman-http-request-pre-request.instructions.md": true
  },
  "markdown.extension.list.toggle.candidate-markers": [
    
    "-",
    "*",
    "+",
    "1.",
    "1)"
  ],
  "vim.handleKeys": {
  "j": true,
  "k": true
  },
  "markdown.extension.listEditing.enabled": false,
}